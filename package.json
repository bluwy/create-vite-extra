  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@clack/prompts": "^0.10.1",
    "create-vite-extra": "link:",
    "cross-spawn": "^7.0.6",
    "minimist": "^1.2.8",
    "mri": "^1.2.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
